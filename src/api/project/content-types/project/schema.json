{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "contents": {
      "type": "component",
      "repeatable": true,
      "component": "general.case-study-row"
    },
    "url": {
      "type": "string"
    },
    "slogen": {
      "type": "string",
      "required": true
    },
    "year": {
      "type": "integer",
      "required": true
    },
    "role": {
      "type": "string",
      "required": true
    },
    "ask": {
      "type": "string",
      "required": true
    },
    "header_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "next_project": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project.project"
    }
